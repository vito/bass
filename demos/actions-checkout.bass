(import (load (*dir*/secrets))
        *access-token*)

(import (load (.actions {:GITHUB_REPOSITORY "vito/bass"
                         :GITHUB_TOKEN *access-token*}))
        load-action
        run-action)

(emit
  (:workspace
    (run-action "actions/checkout@v2"
                :repository "vito/bass"
                :token *access-token*))
  *stdout*)

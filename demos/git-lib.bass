(def stdlib
  (-> ($ .git clone "https://github.com/vito/bass" ./repo/)
      (in-image "bitnami/git")
      (response-from :exit)
      (path ./repo/std/)))

(import (load (stdlib/concourse))
        resource)

(def booklit
  (resource :git {:uri "https://github.com/vito/booklit"}))

(emit (last (booklit:check)) *stdout*)

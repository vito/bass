(def thunk-arg
  (from (linux/alpine)
    ($ cat *dir*/host-paths/one)))

(def thunk-dir
  (from (linux/alpine)
    (cd *dir*/host-paths/
      ($ cat ./two ./three))))

(let [arg-stream (read thunk-arg :json)
      dir-stream (read thunk-dir :json)]
  [(next arg-stream)
   (next dir-stream)
   (next dir-stream)])

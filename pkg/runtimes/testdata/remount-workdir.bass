(def *memos* *dir*/memos.json)

(def initial
  (-> ($ touch ./foo)
      (with-image (linux/alpine))
      (subpath ./)))

(def remount
  (-> ($ touch ./bar)
      (with-image (linux/alpine))
      (with-mount initial ./)))

(def from-remount
  (from remount
    ($ touch ./baz)))

(next (read (from from-remount ($ ls)) :raw))

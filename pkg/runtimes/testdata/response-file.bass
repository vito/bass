(def values
  [null false true 42 "hello" [] [0 "one" -2] {} {:foo "bar"}])

(def create-file
  (-> ($ sh -c "cat > response.json")
      (with-stdin & values)
      (with-image (linux/alpine))))

(def response
  (read create-file/response.json :json))

; TODO: %(next response)?
(map (fn _ (next response)) values)

(defn cat paths
  (-> ($ cat & $paths)
      (response-from :stdout)))

(def thunk-arg
  (from (linux/alpine)
    (cat *dir*/host-paths/one)))

(def thunk-dir
  (from (linux/alpine)
    (cd *dir*/host-paths/
      (cat ./two ./three))))

(let [arg-stream (thunk-arg)
      dir-stream (thunk-dir)]
  [(next arg-stream)
   (next dir-stream)
   (next dir-stream)])

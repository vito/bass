(def values
  [null false true 42 "hello" [] [0 "one" -2] {} {:foo "bar"}])

(def response
  (run (-> ($ sh -c "cat > response.json")
           (with-stdin & values)
           (response-from ./response.json)
           (in-image "alpine"))))

; TODO: %(next response)?
(map (fn _ (next response)) values)
